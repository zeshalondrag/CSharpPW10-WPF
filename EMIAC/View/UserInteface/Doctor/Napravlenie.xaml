<UserControl x:Class="EMIAC.View.UserInteface.Doctor.Napravlenie"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"  
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:EMIAC.View.UserInteface.Doctor"
             mc:Ignorable="d" 
             d:DesignHeight="40" d:DesignWidth="960">
    <Grid>
        <StackPanel  Grid.Row="9" Grid.Column="1" Background="White" Height="40" VerticalAlignment="Top" Grid.ColumnSpan="4">
            <TextBlock Text="Направление к специалисту: " Width="225" Height="39" FontSize="16" HorizontalAlignment="Left"/>
            <materialDesign:PackIcon Foreground="#FE0000" Kind="TrashCanOutline" Width="40" Height="38" Margin="920,-40,0,0"/>
            <Button Background="Transparent" Foreground="Transparent" BorderBrush="Transparent" Width="40" Height="40" Margin="920,-38,10,0"/>
        </StackPanel>
    </Grid>
</UserControl>
